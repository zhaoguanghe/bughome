<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bughome.springboot.mapper.UserMapper">
    <select id="selectUserById" resultType="com.bughome.springboot.model.User">
        select * from t_user where Id = #{id}
    </select>
